<Project>
  <PropertyGroup Label="SDK Versions">
    <NetStandardTargetVersion21>netstandard2.1</NetStandardTargetVersion21>
    <NetCoreTargetVersion>netcoreapp3.1</NetCoreTargetVersion>
    <LangVersion>preview</LangVersion>
    <RazorLangVersion>3.0</RazorLangVersion>
  </PropertyGroup>

  <PropertyGroup Label="Package information">
    <PackageLicenseUrl>https://github.com/Xabaril/Balea/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>http://github.com/xabaril/Balea</PackageProjectUrl>
    <RepositoryUrl>https://github.com/xabaril/Balea</RepositoryUrl>
    <Authors>Xabaril Contributors</Authors>
    <Company>Xabaril</Company>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>true</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>

  <PropertyGroup Label="Balea Versions">
    <Balea>3.0.0$(VersionSuffix)</Balea>
  </PropertyGroup>

  <PropertyGroup Label="Package Dependencies">
    <AspNetCoreVersion>3.1.3</AspNetCoreVersion>
    <EntityFrameworkVersion>3.1.3</EntityFrameworkVersion>
  </PropertyGroup>

  <PropertyGroup Label="Testing Dependencies">
    <MicrosoftNETTestSdk>16.5.0</MicrosoftNETTestSdk>
    <xunit>2.4.1</xunit>
    <xunitrunnervisualstudio>2.4.1</xunitrunnervisualstudio>
    <coverletcollector>1.2.0</coverletcollector>
    <AcheveTestHost>3.0.0</AcheveTestHost>
    <FluentAssertions>5.10.3</FluentAssertions>
    <MicrosoftAspNetCoreTestHost>3.1.3</MicrosoftAspNetCoreTestHost>
    <Respawn>3.3.0</Respawn>
    <AutoFixtureXunit2>4.11.0</AutoFixtureXunit2>
  </PropertyGroup>

  <PropertyGroup Label="CLI Tools Versions">

  </PropertyGroup>
  
</Project>